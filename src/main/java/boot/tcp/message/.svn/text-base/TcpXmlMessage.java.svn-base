package boot.tcp.message;

import boot.pkg.EpccfePkg;

/**
 * @author RockChan
 * @date 2017-08-23
 * @version 1.0
 */
public class TcpXmlMessage {
	private EpccfePkg pkg;

	public TcpXmlMessage(Object pkg) {
		this.pkg = (EpccfePkg)pkg;
	}

	/**
	 * @return the pkg
	 */
	public final EpccfePkg getPkg() {
		return pkg;
	}

	/**
	 * @param pkg the pkg to set
	 */
	public final void setPkg(EpccfePkg pkg) {
		this.pkg = pkg;
	}

	/*
	 * (non-Javadoc)
	 * 
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "TcpXmlMessage [pkg =" + pkg + "]";
	}
}
